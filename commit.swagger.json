{
  "openapi": "3.0.3",
  "info": {
    "title": "代码库Commit API文档",
    "description": "代码库提交（Commit）相关的API文档",
    "version": "1.0.0"
  },
  "tags": [
    {
      "name": "提交",
      "description": "Codeup代码库提交（Commit）相关的接口"
    }
  ],
  "paths": {
    "/codeup/organizations/{organizationId}/repositories/{repositoryId}/commits": {
      "get": {
        "tags": [
          "提交"
        ],
        "summary": "查询提交列表",
        "description": "查询代码库提交（Commit）列表",
        "operationId": "ListCommits",
        "parameters": [
          {
            "name": "organizationId",
            "in": "path",
            "description": "组织ID",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "60d54f3daccf2bbd6659f3ad"
          },
          {
            "name": "repositoryId",
            "in": "path",
            "description": "代码库ID或者URL-Encoder编码的全路径",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "2813489或者60de7a6852743a5162b5f957%2FDemoRepo"
          },
          {
            "name": "refName",
            "in": "query",
            "description": "分支名称、标签名称或提交版本，默认为代码库默认分支",
            "required": true,
            "style": "form",
            "schema": {
              "type": "string"
            },
            "example": "master"
          },
          {
            "name": "since",
            "in": "query",
            "description": "提交起始时间，格式：YYYY-MM-DDTHH:MM:SSZ",
            "required": false,
            "style": "form",
            "schema": {
              "type": "string"
            },
            "example": "2024-10-05T15:30:45Z"
          },
          {
            "name": "until",
            "in": "query",
            "description": "提交截止时间，格式：YYYY-MM-DDTHH:MM:SSZ",
            "required": false,
            "style": "form",
            "schema": {
              "type": "string"
            },
            "example": "2024-10-05T15:30:45Z"
          },
          {
            "name": "page",
            "in": "query",
            "description": "页码",
            "required": false,
            "style": "form",
            "schema": {
              "type": "integer",
              "format": "int64"
            },
            "example": "1"
          },
          {
            "name": "perPage",
            "in": "query",
            "description": "每页大小",
            "required": false,
            "style": "form",
            "schema": {
              "type": "integer",
              "format": "int64"
            },
            "example": "10"
          },
          {
            "name": "path",
            "in": "query",
            "description": "文件路径",
            "required": false,
            "style": "form",
            "schema": {
              "type": "string"
            },
            "example": "src/test/main.java"
          },
          {
            "name": "search",
            "in": "query",
            "description": "搜索关键字",
            "required": false,
            "style": "form",
            "schema": {
              "type": "string"
            },
            "example": "Test"
          },
          {
            "name": "showSignature",
            "in": "query",
            "description": "是否展示签名",
            "required": false,
            "style": "form",
            "schema": {
              "type": "boolean"
            },
            "example": "false"
          },
          {
            "name": "committerIds",
            "in": "query",
            "description": "提交人ID列表（多个ID以逗号隔开）",
            "required": false,
            "style": "form",
            "schema": {
              "type": "string"
            },
            "example": ["62c795xxxb468af8"]
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/DevopsCommitVO"
                  }
                }
              }
            },
            "headers": {
              "x-next-page": {
                "type": "integer",
                "description": "下一页",
                "format": "int64",
                "example": "3"
              },
              "x-page": {
                "type": "integer",
                "description": "当前页",
                "format": "int64",
                "example": "2"
              },
              "x-per-page": {
                "type": "integer",
                "description": "每页大小",
                "format": "int64",
                "example": "20"
              },
              "x-prev-page": {
                "type": "integer",
                "description": "前一页",
                "format": "int64",
                "example": "1"
              },
              "x-total": {
                "type": "integer",
                "description": "总数",
                "format": "int64",
                "example": "50"
              },
              "x-total-pages": {
                "type": "integer",
                "description": "总分页数",
                "format": "int64",
                "example": "3"
              },
              "x-request-id": {
                "type": "string",
                "description": "请求ID",
                "example": "37294673-00CA-5B8B-914F-A8B35511E90A"
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
    "/codeup/organizations/{organizationId}/repositories/{repositoryId}/commits/{sha}": {
      "get": {
        "tags": [
          "提交"
        ],
        "summary": "查询提交信息",
        "description": "查询单个提交信息",
        "operationId": "GetCommit",
        "parameters": [
          {
            "name": "organizationId",
            "in": "path",
            "description": "组织ID",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "60d54f3daccf2bbd6659f3ad"
          },
          {
            "name": "repositoryId",
            "in": "path",
            "description": "代码库ID或者URL-Encoder编码的全路径",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "2813489或者60de7a6852743a5162b5f957%2FDemoRepo"
          },
          {
            "name": "sha",
            "in": "path",
            "description": "提交ID，即Commit SHA值",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "6da8c14b5a9102998148b7ea35f96507d5304f74"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/DevopsCommitVO"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
    "/codeup/organizations/{organizationId}/repositories/{repositoryId}/commits/{sha}/comments": {
      "post": {
        "tags": [
          "提交"
        ],
        "summary": "给单个提交添加评论",
        "description": "给单个提交添加评论",
        "operationId": "CreateCommitComment",
        "parameters": [
          {
            "name": "organizationId",
            "in": "path",
            "description": "组织ID",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "60de7a6852743a5162b5f957"
          },
          {
            "name": "repositoryId",
            "in": "path",
            "description": "代码库ID或者URL-Encoder编码的全路径",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            },
            "example": "2813489或者60de7a6852743a5162b5f957%2FDemoRepo"
          },
          {
            "description": "提交的SHA值",
            "example": "2b4f8fc38bdf464359c3a05334654fa27e15a704",
            "in": "path",
            "name": "sha",
            "required": true,
            "schema": {
              "example": "2b4f8fc38bdf464359c3a05334654fa27e15a704",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateCommitCommentVO"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "删除应用标签成功"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "DevopsCommitVO": {
        "title": "提交",
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "提交ID",
            "example": "6da8c14b5a9102998148b7ea35f96507d5304f74"
          },
          "shortId": {
            "type": "string",
            "description": "代码组路径",
            "example": "6da8c14b"
          },
          "title": {
            "type": "string",
            "description": "标题，提交的第一行内容",
            "example": "commit msg title"
          },
          "message": {
            "type": "string",
            "description": "提交内容",
            "example": "commit message detail"
          },
          "authorName": {
            "type": "string",
            "description": "作者姓名",
            "example": "codeup-name"
          },
          "authorEmail": {
            "type": "string",
            "description": "作者邮箱",
            "example": "username@example.com"
          },
          "authoredDate": {
            "type": "string",
            "description": "作者提交时间",
            "example": "2024-10-05T15:30:45Z"
          },
          "committerName": {
            "type": "string",
            "description": "提交者姓名",
            "example": "codeup-name"
          },
          "committerEmail": {
            "type": "string",
            "description": "提交者邮箱",
            "example": "username@example.com"
          },
          "committedDate": {
            "type": "string",
            "description": "提交者提交时间",
            "example": "2024-10-05T15:30:45Z"
          },
          "webUrl": {
            "type": "string",
            "description": "页面访问地址",
            "example": "http://exmaple.com/example_repo/commit/commit_sha"
          },
          "parentIds": {
            "type": "array",
            "description": "父提交ID",
            "items": {
              "type": "string"
            },
            "example": ["3fdaf119cf76539c1a47de0074ac02927ef4c8e1"]
          }
        }
      },
      "DevopsCommitStatVO": {
        "title": "DevopsCommitStatVO",
        "type": "object",
        "description": "变更行数统计",
        "properties": {
          "additions": {
            "type": "integer",
            "description": "增加行数",
            "format": "int64",
            "example": "1"
          },
          "deletions": {
            "type": "integer",
            "description": "删除行数",
            "format": "int64",
            "example": "1"
          },
          "total": {
            "type": "integer",
            "description": "总变动行数",
            "format": "int64",
            "example": "2"
          }
        }
      },
      "CreateCommitCommentVO": {
        "title": "CreateCommitCommentVO",
        "type": "object",
        "description": "commit的评论",
        "properties": {
          "content": {
            "type": "string",
            "description": "commit的评论内容",
            "example": "comment content"
          }
        },
        "required": ["content"]
      }
    }
  }
}